<% layout("/layouts/boilerplate") %>
<body>
<form method="POST" action="/listings/<%= lists._id %>?_method=PUT">
<input name="listing[title]" value="<%= lists.title %>" type="text"/>
<br/><br/><br/>
<textarea name="listing[description]" value="<%= lists.description %>" type="text"></textarea>
<br/><br/><br/>
<input name="listing[image]" value="<%= lists.image %>" type="text"/>
<br/><br/><br/>
<input name="listing[price]" value="<%= lists.price %>" type="number"/>
<br/><br/><br/>
<input name="listing[location]" value="<%= lists.location %>" type="text"/>
<br/><br/><br/>
<button>EDIT</button>

</form>


</body>
